<div class="close-more-data hide" id="more-data-<%= data.sdid %>">
  <span class="icon" data-id="<%= data.sdid %>">
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" id="angle-double-left">
      <path fill="#"
        d="M11.46,8.29a1,1,0,0,0-1.42,0l-3,3a1,1,0,0,0,0,1.42l3,3a1,1,0,0,0,1.42,0,1,1,0,0,0,0-1.42L9.16,12l2.3-2.29A1,1,0,0,0,11.46,8.29ZM14.66,12,17,9.71a1,1,0,0,0-1.42-1.42l-3,3a1,1,0,0,0,0,1.42l3,3a1,1,0,0,0,1.42,0,1,1,0,0,0,0-1.42Z">
      </path>
    </svg>
  </span>
   <div class="more-data-text align-center" style="opacity: .9;">
    <h2>
      <%= data.work_name %>
    </h2>
    <h2>
   <span style="opacity: .7;">Reference no:</span>
   <%= data.reference_no %>
    </h2>
    <h3><span style="opacity: .7;">Category: </span><%= data.category ? data.category : "N/A" %></h3>
   
  </div>
  <!-- DETAILS  -->
  <div class="contents grid">
    <div class="description flex align-center">
      <h3>Client Name:</h3>
      <p class="text">
        <%= data.sdeal_name %>
      </p>
    </div>
    <div class="description flex align-center">
      <h3>Phone Number:</h3>
      <p class="text">+91 <%= data.contact %>
      </p>
    </div>
    <div class="description flex align-center">
      <h3>Email ID:</h3>
      <p class="text">
        <%= data.email %>
      </p>
    </div>
    <div class="description flex align-center">
      <h3>Address:</h3>
      <p class="text">
        <%= data.city %>
      </p>
    </div>
    <div class="description flex align-center">
      <h3>Total Project Amount(Value):</h3>
      <p class="text">&#8377; <span>
          <%= data.total_price? data.total_price :"0/-"  %>
        </span></p>
    </div>
    <div class="description flex align-center">
      <h3>Agreement Date:</h3>
      <p class="text">
        <%= data.mp_deadline ? data.mp_deadline : "N/A" %>
      </p>
    </div>
  </div>

  <!-- --BOX-- -->
  <div class="task-grid grid">
    <%- include('../partials/tasks/subtask.ejs', {subdata: data}) %>
  </div>
</div>