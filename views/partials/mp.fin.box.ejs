<div class="accordion-content">
    <div class="ahead misgrid">
        <p class="ref">
            <span class="uppercase hide block phead">Ref. No.</span>
            <span><%= data.reference_no %></span>
        </p>
        <p class="pro">
            <span class="uppercase hide block phead">name of work</span>
            <span><%= data.work_name %></span>
        </p>
        <p class="emp_date">
            <span class="uppercase hide block phead">location</span>
            <span><%= data.city %></span>
        </p>
        <p class="totalf">
            <span class="uppercase hide block phead">Total</span>
            &#8377; <%= data.total_price %><span></span>/-
        </p>
        <p class="totalf" onclick="advOpen(this)" data-mdealid="<%= data.sdid %>" data-taskid="<%= data.task %>">
            <span class="uppercase hide block phead">advance</span>
            &#8377;<span><%= data.agreement_amount %></span>/- <span class="maka"><%= data.modeofpay && data.modeofpay %></span>
        </p>
        <p class="totalf">
            <span class="uppercase hide block phead">Received</span>
            &#8377;<span><%= data.total_price - data.agreement_amount %></span>/-
        </p>
