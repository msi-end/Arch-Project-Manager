<div class="architecture badge">
    <div class="arrow">
        <p class="uppercase phead"><%= cat.name %></p>
        <div class="task-emp-status">
            <div class="main-btns flex">
                <button class="status-btn flex" onclick="ChangeTaskStatus(this)">
                    <span class="status_project">Status</span>
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"
                        id="arrow-circle-down">
                        <path fill="##000000"
                            d="M11.29,15.71a1,1,0,0,0,.33.21,1,1,0,0,0,.76,0,1,1,0,0,0,.33-.21l3-3a1,1,0,0,0-1.42-1.42L13,12.59V9a1,1,0,0,0-2,0v3.59l-1.29-1.3a1,1,0,0,0-1.42,0,1,1,0,0,0,0,1.42ZM12,22A10,10,0,1,0,2,12,10,10,0,0,0,12,22ZM12,4a8,8,0,1,1-8,8A8,8,0,0,1,12,4Z">
                        </path>
                    </svg>
                </button>
                <button class="" onclick="empPopup()">Employee</button>
                <button class="" onclick="subPopup()">Tasks</button>
            </div>
            <div class="main-dropdown">
                <div class="status-dropdown-menu">
                    <ul>
                        <li class="red">
                            <input type="radio" name="start">
                            <span data-name="start">Not Started</span>
                        </li>
                        <li class="blue">
                            <input type="radio" name="start">
                            <span>In Progress</span>
                        </li>
                        <li class="green">
                            <input type="radio" name="start">
                            <span>Completed</span>
                        </li>
                    </ul>
                </div>
                <div class="emp-drop-menu">
                    <ul>
                        <li>
                            <input type="checkbox" name="employee">
                            <span>Anand Kumar</span>
                        </li>
                        <li>
                            <input type="checkbox" name="employee">
                            <span>Vishal Langthasa</span>
                        </li>
                        <li>
                            <input type="checkbox" name="employee">
                            <span>Gaurav Mukherjee</span>
                        </li>
                        <li>
                            <input type="checkbox" name="employee">
                            <span>Aryan Kahdri</span>
                        </li>
                        <li>
                            <input type="checkbox" name="employee">
                            <span>Vashu Saksena</span>
                        </li>
                        <li>
                            <input type="checkbox" name="employee">
                            <span>Surya Singham</span>
                        </li>
                        <li>
                            <input type="checkbox" name="employee">
                            <span>Khatam Tata</span>
                        </li>
                    </ul>
                    <button class="uppercase">update</button>
                </div>
                <div class="task-drop-menu">
                    <ul>
                        <li class="stasks">
                            <input type="checkbox" name="plumbing" id="">
                            <span>Plumbing</span>
                        </li>
                        <li class="stasks">
                            <input type="checkbox" name="electricity" id="">
                            <span>Electricity</span>
                        </li>
                    </ul>
                    <button class="uppercase">update</button>
                </div>

            </div>
        </div>
    </div>

    <div class="sub-tasks sbox">
        <div class="assign-to">
            <div class="eaccordion flex">
                <p class="uppercase phead">assigned to</p>
                <span class="right-arr">
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" id="angle-double-right" class="svg"><path fill="##000000" d="M8.46,8.29A1,1,0,1,0,7,9.71L9.34,12,7,14.29a1,1,0,0,0,0,1.42,1,1,0,0,0,1.42,0l3-3a1,1,0,0,0,0-1.42Zm8.5,3-3-3a1,1,0,0,0-1.42,1.42L14.84,12l-2.3,2.29a1,1,0,0,0,0,1.42,1,1,0,0,0,1.42,0l3-3A1,1,0,0,0,17,11.29Z"></path></svg>
                </span>
            </div>
            <ul class="emp-acc-data">
                <li class="add-empl">Sri Venkataraman Venugopal Iyer</li>
                <li class="add-empl">Sri Venkataraman Venugopal Iyer</li>
            </ul>
        </div>
        <hr class="show">
        <div class="tasks-added">
            <p class="uppercase phead">tasks</p>
            <ul>
                <% cat.sub?.forEach((el)=>{ 
                if(el){  %>
                <li class="stasks">
                    <input type="checkbox" name="plumbing" class="" <%= `checked`%>>
                    <span><%= el %></span>
                </li>
                <% }}) %>
            </ul>
        </div>
    </div>
</div>