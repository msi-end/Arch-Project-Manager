<div class="accordion-content">
    <div class="ahead grid">
        <p class="ref">
            <span class="uppercase hide block phead">Ref. No.</span>
            <span><%= data.reference_no %></span>
        </p>
        <p class="cli none"><%= data.sdeal_name %></p>
        <p class="pro">
            <span class="uppercase hide block phead">name of work</span>
            <span><%= data.work_name %></span>
        </p>
        <p class="emp_date">
            <span class="uppercase hide block phead">Deadline</span>
            <span><%= data.reference_no %></span>
        </p>
        <p class="stat flex red"><%= data.reference_no %></p>
        <p class="arrow-down">
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" id="angle-double-down" class="svg">
                <path fill="##000000"
                    d="M11.29,11.46a1,1,0,0,0,1.42,0l3-3A1,1,0,1,0,14.29,7L12,9.34,9.71,7A1,1,0,1,0,8.29,8.46Zm3,1.08L12,14.84l-2.29-2.3A1,1,0,0,0,8.29,14l3,3a1,1,0,0,0,1.42,0l3-3a1,1,0,0,0-1.42-1.42Z">
                </path>
            </svg>
        </p>
    </div>
    <!-- ----------open data for accordion---------- -->
    <div class="adata">
        <div class="oth-info grid none">
            <p></p>
            <p class="contact">
                <span class="uppercase phead">contact No</span>
                <span class="contact_no"><%= data.contact %></span>
            </p>
            <p class="e-mail">
                <span class="uppercase phead">email</span>
                <span class="g-mail"><%= data.email %></span>
            </p>
            <p class="agreement">
                <span class="uppercase phead">Agreement Amount</span>
                <span class="amount">&#8377;<%= data.agreement_amount %></span>
            </p>
            <p class="address">
                <span class="uppercase phead">address</span>
                <span class="add_info"><%= data.work_name %></span>
            </p>
            <p></p>
        </div>
        <div class="project_expand grid">
            <p class="none"></p>
            <!-- ARCHITECTURE  -->
            <div class="architecture badge">
                <div class="arrow">
                    <p class="uppercase phead">Status</p>
                    <div class="task-emp-status">
                        <div class="main-btns flex">
                            <select data-taskstatus="" class="status-dropdown-menu common_dropdown" name="status" id="">
                                <option value="Not Started">Not started</option>
                                <option value="On Progress">On Progress</option>
                                <option value="Completed">Completed</option>
                            </select>
                            <!-- <button class="" onclick="empPopup()">Employee</button> -->
                        </div>

                    </div>
                </div>
                <div class="sub-tasks sbox">
                    <div class="assign-to">
                        <div class="eaccordion flex">
                            <p class="uppercase phead">assigned to</p>
                            <span class="right-arr">
                                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" id="angle-double-right"
                                    class="svg">
                                    <path fill="##000000"
                                        d="M8.46,8.29A1,1,0,1,0,7,9.71L9.34,12,7,14.29a1,1,0,0,0,0,1.42,1,1,0,0,0,1.42,0l3-3a1,1,0,0,0,0-1.42Zm8.5,3-3-3a1,1,0,0,0-1.42,1.42L14.84,12l-2.3,2.29a1,1,0,0,0,0,1.42,1,1,0,0,0,1.42,0l3-3A1,1,0,0,0,17,11.29Z">
                                    </path>
                                </svg>
                            </span>
                        </div>
                        <ul class="emp-acc-data">
                            <li class="add-empl">Sri Venkataraman Venugopal Iyer</li>
                            <li class="add-empl">Sri Venkataraman Venugopal Iyer</li>
                        </ul>
                        <button class="" onclick="empPopup()">Employee</button>
                    </div>
                </div>
            </div>
            <div class="architecture badge">
                <div class="arrow">
                    <p class="uppercase phead">Employee</p>
                    <div class="task-emp-status">
                        <div class="main-btns flex">
                            <select data-taskstatus="" class="status-dropdown-menu common_dropdown" name="status" id="">
                                <option value="Not Started">Not started</option>
                                <option value="On Progress">On Progress</option>
                                <option value="Completed">Completed</option>
                            </select>
                            <!-- <button class="" onclick="empPopup()">Employee</button> -->
                        </div>

                    </div>
                </div>
                <div class="sub-tasks sbox">
                    <div class="assign-to">
                        <div class="eaccordion flex">
                            <p class="uppercase phead">assigned to</p>
                            <span class="right-arr">
                                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" id="angle-double-right"
                                    class="svg">
                                    <path fill="##000000"
                                        d="M8.46,8.29A1,1,0,1,0,7,9.71L9.34,12,7,14.29a1,1,0,0,0,0,1.42,1,1,0,0,0,1.42,0l3-3a1,1,0,0,0,0-1.42Zm8.5,3-3-3a1,1,0,0,0-1.42,1.42L14.84,12l-2.3,2.29a1,1,0,0,0,0,1.42,1,1,0,0,0,1.42,0l3-3A1,1,0,0,0,17,11.29Z">
                                    </path>
                                </svg>
                            </span>
                        </div>
                        <ul class="emp-acc-data">
                            <li class="add-empl">Sri Venkataraman Venugopal Iyer</li>
                            <li class="add-empl">Sri Venkataraman Venugopal Iyer</li>
                        </ul>
                        <button class="" onclick="empPopup()">Employee</button>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>
<div class="main-dropdown hide">
    <div class="emp-drop-menu common_dropdown">
        <ul>
            <li>
                <input type="checkbox" name="employee">
                <span>Anand Kumar</span>
            </li>
            <li>
                <input type="checkbox" name="employee">
                <span>Vishal Langthasa</span>
            </li>
            <li>
                <input type="checkbox" name="employee">
                <span>Gaurav Mukherjee</span>
            </li>
        </ul>
        <button class="uppercase">update</button>
    </div>
</div>