            <!-- ----------- DATA TABLE----------- -->

            <div class="data-info expense-table">
              <table>
                <thead>
                  <tr>
                    <th>Ref No.</th>
                    <th>Title</th>
                    <th>Amount</th>
                    <th>Date</th>
                    <th>Mode of Pay</th>
                    <th>Category</th>
                    <th>Remarks</th>
                    <th>Action</th>
                  </tr>
                </thead>

                <tbody id="dataTable">
                  <% data?.forEach(exps=>{ %>
                    <tr data-exps_id="<%= exps.id %>" data-type="<%= exps.type %>" class="<%= exps.type=="expense"?"expense-border":"income-border" %> ">
                      <td>
                        <%= exps.id %>
                      </td>
                      <td class="exp-name-data">
                        <%= exps.title %>
                      </td>
                      <td class="exp-amount-data">
                        &#8377; <%= exps.amount %>
                      </td>
                      <td class="exp-date-data">
                        <%= exps.date %>
                      </td>
                      <td class="exp-mode-data">
                        <%= exps.mode_of_pay %>
                      </td>
                      <td class="exp-category">
                        <%= exps.category? exps.category:'N/A' %>
                      </td>
                      <td class="exp-remark">
                        <%= exps.remark? exps.remark : 'No remarks' %>
                      </td>
                      <td class="flex align-center">
                        <button class="edit" data-exps_id="<%= exps.id %>" onclick="editExpense(this)">Edit</button>
                      </td>
                    </tr>
                    <% }) %>
                </tbody>

                <tfoot>
                  <tr>
                    <td colspan="4"></td>
                    <td colspan="4"></td>
                  </tr>
                </tfoot>
              </table>
            </div>
        </div>



        <!-- POPUPS  -->

        <div class="main-popup hide"></div>
        <%- include('../partials/tasks/notification.ejs') %>
          <%- include('../partials/tasks/alert.ejs') %>
      </main>
  </section>


